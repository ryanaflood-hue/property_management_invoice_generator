{
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "crons": [
        {
            "path": "/run-today",
            "schedule": "0 6 * * *"
        }
    ]
}
